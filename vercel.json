{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "vercel-sapper"
        },
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "api/index.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "api/index.js"
        }
    ]
}